
! ROUTER-2: SERVIDOR DHCP
! 
enable
configure terminal

hostname R2-DHCP


interface GigabitEthernet0/0
 description CONEXION A SWITCH - DHCP SERVER
 ip address 20.24.116.4 255.255.255.0
 no shutdown
 exit

ip route 0.0.0.0 0.0.0.0 20.24.116.1


! CONFIGURACIÓN DHCP CON TU MATRÍCULA 202.x.x.x



ip dhcp excluded-address 20.24.116.1      ! Router Gateway
ip dhcp excluded-address 20.24.116.2      ! Kali Atacante
ip dhcp excluded-address 20.24.116.3      ! Windows Víctima
ip dhcp excluded-address 20.24.116.4      ! Este router (DHCP)
ip dhcp excluded-address 20.24.116.254    ! Broadcast

! POOL PRINCIPAL - PARA VÍCTIMAS LEGÍTIMAS
ip dhcp pool POOL_LEGITIMO_20241165
 network 20.24.116.0 255.255.255.0
 default-router 20.24.116.1
 dns-server 8.8.8.8 1.1.1.1
 domain-name practica20241165.local
 lease 1 0 0
 exit


ip dhcp pool POOL_RESERVA
 network 20.24.116.0 255.255.255.0
 default-router 20.24.116.1
 dns-server 8.8.8.8
 lease 1
 exit


do show ip interface brief
do show ip dhcp pool
do show ip route

end
write memory

write memory
